{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOW-FROM https://www.hissuma.com https://hissuma.com" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://www.hissuma.com https://hissuma.com" }
      ]
    }
  ]
}
